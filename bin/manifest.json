{
	"name": "Process Killer",
	"version": "0.1",
	"manifest_version": 2,
	"description": "Chrome のプロセスを終了させ、メモリを節約する拡張機能です。",
	"icons": {
		"64": "icon64.png",
		"48": "icon48.png",
		"128": "icon128.png",
		"32": "icon32.png",
		"16": "icon16.png"
	},
	"browser_action": {
		"default_icon": "icon128.png",
		"default_popup": "popup.html",
		"default_title": "Process Killer"
	},
	"permissions": [
		"experimental",
		"tabs",
		"chrome://favicon/"
	]
}